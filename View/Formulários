                ReadOnly = true,
                DefaultCellStyle = new DataGridViewCellStyle
                {
                    Alignment = DataGridViewContentAlignment.MiddleLeft,
                    Font = new Font("Segoe UI", 9F, FontStyle.Regular)
                },
                HeaderCell = { Style = { Alignment = DataGridViewContentAlignment.MiddleCenter } }
            });

            // üîπ Coluna Qtde (CENTRALIZADA)
            dgvitens.Columns.Add(new DataGridViewTextBoxColumn
            {
                HeaderText = "Qtde",
                DataPropertyName = nameof(ItemVendaModel.Quantidade),
                Width = 80,
                DefaultCellStyle = new DataGridViewCellStyle
                {
                    Alignment = DataGridViewContentAlignment.MiddleCenter, // üîπ CENTRALIZADO
                    Font = new Font("Segoe UI", 9F, FontStyle.Regular),
                    BackColor = Color.WhiteSmoke, // üîπ Fundo diferenciado para campo edit√°vel
                    SelectionBackColor = Color.LightBlue
                },
                HeaderCell = { Style = { Alignment = DataGridViewContentAlignment.MiddleCenter } }
            });

            // üîπ Coluna Pre√ßo
            dgvitens.Columns.Add(new DataGridViewTextBoxColumn
            {
                HeaderText = "Pre√ßo Unit.",
                DataPropertyName = nameof(ItemVendaModel.PrecoUnitario),
                Width = 100,
                DefaultCellStyle = new DataGridViewCellStyle
                {
                    Format = "N2",
                    Alignment = DataGridViewContentAlignment.MiddleRight, // üîπ Alinhado √† direita
                    Font = new Font("Segoe UI", 9F, FontStyle.Regular)
                },
                HeaderCell = { Style = { Alignment = DataGridViewContentAlignment.MiddleCenter } }
            });

            // üîπ Coluna Subtotal
            dgvitens.Columns.Add(new DataGridViewTextBoxColumn
            {
                HeaderText = "Subtotal",
                DataPropertyName = nameof(ItemVendaModel.Subtotal),
                Width = 130,
                ReadOnly = true,
                DefaultCellStyle = new DataGridViewCellStyle
                {
                    Format = "N2",
                    Alignment = DataGridViewContentAlignment.MiddleRight, // üîπ Alinhado √† direita
                    Font = new Font("Segoe UI", 9F, FontStyle.Bold), // üîπ Negrito para valor importante
                    ForeColor = Color.DarkGreen, // üîπ Cor verde para valores monet√°rios
                    BackColor = Color.LightYellow // üîπ Fundo diferenciado
                },
                HeaderCell = { Style = { Alignment = DataGridViewContentAlignment.MiddleCenter } }
            });

            // üîπ Coluna Remover
            dgvitens.Columns.Add(new DataGridViewButtonColumn
            {
                Text = "‚úï", // üîπ S√≠mbolo mais limpo
                UseColumnTextForButtonValue = true,
                Width = 40,
                FlatStyle = FlatStyle.Flat,
                DefaultCellStyle = new DataGridViewCellStyle
                {
                    Alignment = DataGridViewContentAlignment.MiddleCenter,
                    Font = new Font("Segoe UI", 9F, FontStyle.Bold),
                    ForeColor = Color.White,
                    BackColor = Color.FromArgb(192, 0, 0), // üîπ Vermelho escuro
                    SelectionBackColor = Color.FromArgb(220, 0, 0)
                },
                HeaderText = "Remover",
                HeaderCell = { Style = { Alignment = DataGridViewContentAlignment.MiddleCenter } }
            });

            // üîπ Configura√ß√µes gerais do Grid
            dgvitens.AllowUserToAddRows = false;
            dgvitens.SelectionMode = DataGridViewSelectionMode.FullRowSelect;
            dgvitens.MultiSelect = false; // üîπ S√≥ permite selecionar uma linha por vez
            dgvitens.EnableHeadersVisualStyles = false;
            dgvitens.ColumnHeadersHeightSizeMode = DataGridViewColumnHeadersHeightSizeMode.DisableResizing;
            dgvitens.ColumnHeadersHeight = 30; // üîπ Altura padr√£o
            dgvitens.RowHeadersVisible = false;
            dgvitens.AutoSizeColumnsMode = DataGridViewAutoSizeColumnsMode.None;
            dgvitens.AllowUserToResizeRows = false;
            dgvitens.RowTemplate.Height = 28; // üîπ Altura das linhas

            // üîπ Estilo dos cabe√ßalhos
            dgvitens.ColumnHeadersDefaultCellStyle = new DataGridViewCellStyle
            {
                Font = new Font("Segoe UI", 9.5F, FontStyle.Bold),
                BackColor = Color.SteelBlue,
                ForeColor = Color.White,
                Alignment = DataGridViewContentAlignment.MiddleCenter,
                Padding = new Padding(0, 2, 0, 2) // üîπ Espa√ßamento vertical interno
            };

